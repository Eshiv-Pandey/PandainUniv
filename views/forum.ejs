<%- layout("/layouts/boilerplate.ejs") %>
<div class="row show-row">
  <%for(let data of alldata){ %>
  <div class="card col-2" style="width: 18rem;">
    <img src="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcQSB5c481j4Q6IoUj2Yg595fREHScBOoAUzXw&s" class="card-img-top" alt="...">
    <div class="card-body">
      <h5 class="card-title"><%= data.University %></h5>
      <p class="card-text"><%= data.Program %></p>
      <p>Course : <%= data.Degree %><br>
         Season: <%= data.Season %><br>
      </p>
      <p class="badge text-bg-primary"><%= data.Status %></p>
    </div>
  </div>
  <% } %>
</div>
